		"axios-hooks": "^2.1.0",
		"classnames": "^2.2.6",
		"date-fns": "^2.15.0",
		"loglevel": "^1.6.8",
		"node-sass": "^7.0.0",
		"ow": "^0.17.0",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^3.4.3",
		"react-socks": "^2.1.0"
